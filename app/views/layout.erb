          <li class=<%="active" if active_page?("Grouplike")%> >
	    <a href="/Grouplike">Grouplike</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
    
    <!-- YIELD -------------------------------------------- -->
    <div class="container"> <%= yield %> </div>
