    <footer class="footer">
      <div class="container">
	<p class="text-muted"><%= Time.now %></p>
      </div>
    </footer>
    
    <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
    <script src="js/bootstrap.js"></script>
